{
    "framework": "flute",
    "dataset": "default_credit_horizontal",
    "algorithm": "fedavg",
    "model": "mlp_128",
    "bench_param": {
        "mode": "local",
        "device": "cpu"
    },
    "training_param": {
        "epochs": 30,
        "batch_size": 64,
        "learning_rate": 0.01,
        "num_clients_per_iteration": 2,
        "loss_func": "cross_entropy",
        "optimizer": "sgd",
        "optimizer_param": {
            "momentum": 0.9,
            "lr_decay_factor": 1.0
        }
    }
}