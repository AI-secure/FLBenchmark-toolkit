{
    "framework": "flute",
    "dataset": "celeba",
    "algorithm": "fedavg",
    "model": "leaf_cnn",
    "bench_param": {
        "mode": "local",
        "device": "cpu"
    },
    "training_param": {
        "epochs": 500,
        "batch_size": 16,
        "learning_rate": 0.1,
        "num_clients_per_iteration": 20,
        "loss_func": "cross_entropy",
        "optimizer": "sgd",
        "optimizer_param": {
            "momentum": 0.9,
            "lr_decay_factor": 0.99
        }
    }
}