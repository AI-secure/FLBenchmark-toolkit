{
    "framework": "paddlefl",
    "dataset": "femnist",
    "algorithm": "fedavg",
    "model": "mlp_128_128_128",
    "bench_param": {
        "mode": "local",
        "device": "cpu"
    },
    "training_param": {
        "epochs": 2000,
        "batch_size": 8,
        "learning_rate": 0.001,
        "loss_func": "cross_entropy",
        "optimizer": "sgd",
        "inner_step": 1,
        "client_per_round": 20,
        "optimizer_param": {
            "momentum": 0.9,
            "dampening": 0,
            "weight_decay": 0,
            "nesterov": false
        }
    }
}