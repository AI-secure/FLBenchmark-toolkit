{
    "framework": "crypten",
    "dataset": "motor_vertical",
    "algorithm": "mpc",
    "model": "mlp_128_128_128",
    "bench_param": {
        "mode": "local",
        "device": "cpu"
    },
    "training_param": {
        "epochs": 30,
        "batch_size": 64,
        "learning_rate": 0.1,
        "loss_func": "mse_loss",
        "optimizer": "sgd",
        "optimizer_param": {
            "momentum": 0.9,
            "dampening": 0,
            "weight_decay": 0,
            "nesterov": false
        }
    }
}