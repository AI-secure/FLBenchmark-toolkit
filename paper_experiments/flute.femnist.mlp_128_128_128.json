{
    "framework": "flute",
    "dataset": "femnist",
    "algorithm": "fedavg",
    "model": "mlp_128_128_128",
    "bench_param": {
        "mode": "local",
        "device": "cpu"
    },
    "training_param": {
        "epochs": 2000,
        "batch_size": 8,
        "learning_rate": 0.001,
        "num_clients_per_iteration": 20,
        "loss_func": "cross_entropy",
        "optimizer": "sgd",
        "optimizer_param": {
            "momentum": 0.9,
            "lr_decay_factor": 1.0
        }
    }
}