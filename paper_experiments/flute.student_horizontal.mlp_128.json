{
    "framework": "flute",
    "dataset": "student_horizontal",
    "algorithm": "fedavg",
    "model": "mlp_128",
    "bench_param": {
        "mode": "local",
        "device": "cpu"
    },
    "training_param": {
        "epochs": 200,
        "batch_size": 32,
        "learning_rate": 0.0005,
        "num_clients_per_iteration": 2,
        "loss_func": "mse",
        "optimizer": "sgd",
        "optimizer_param": {
            "momentum": 0.9,
            "lr_decay_factor": 1.0
        }
    }
}