        "epochs": 20,
        "batch_size": 8,
        "learning_rate": 0.001,
        "num_clients_per_iteration": 20,
        "loss_func": "cross_entropy",
        "optimizer": "sgd",
        "optimizer_param": {
            "momentum": 0.9
        }
    }
}