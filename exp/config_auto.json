{
    "framework": "flower",
    "algorithm": "fedavg",
    "training": {
        "global_epochs": 25,
        "batch_size": 8,
        "learning_rate": 0.001,
        "client_per_round": 36,
        "loss_func": "cross_entropy",
        "optimizer": "sgd",
        "optimizer_param": {
            "momentum": 0.9,
            "dampening": 0,
            "weight_decay": 0,
            "nesterov": false
        }
    },
    "dataset": "femnist",
    "model": "lenet",
    "deployment": {
        "mode": "colink-default"
    }
}