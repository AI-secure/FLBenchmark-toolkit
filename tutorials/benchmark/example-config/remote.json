{
    "framework": "crypten",
    "dataset": "breast_vertical",
    "algorithm": "mpc",
    "model": "mlp_128",
    "bench_param": {
        "mode": "remote",
        "device": "cpu",
        "hosts": [
            {
                "id": 0,
                "hostname": "10.0.0.1"
            },
            {
                "id": 1,
                "hostname": "10.0.0.2"
            }
        ]
    },
    "training_param": {
        "epochs": 8,
        "batch_size": 100,
        "learning_rate": 0.1,
        "loss_func": "cross_entropy",
        "optimizer": "sgd",
        "optimizer_param": {
            "momentum": 0.9,
            "dampening": 0,
            "weight_decay": 0,
            "nesterov": false
        }
    }
}